		<xs:element name="ADDRESS-2" type="xs:string" minOccurs="0" maxOccurs="1"/>
		<xs:element name="ADDRESS-3" type="xs:string" minOccurs="0" maxOccurs="1"/>
		-->
		<xs:element ref="PHONES" minOccurs="1" maxOccurs="1"/>
		<!--
		<xs:element name="PHONE-1" type="xs:string" minOccurs="0" maxOccurs="1"/>
