		<xs:element name="CREDIT-INQUIRY-STAGE" type="xs:string" minOccurs="0" maxOccurs="1"/>
		<xs:element name="CREDIT-RPT-ID" type="xs:string" minOccurs="0" maxOccurs="1"/>
