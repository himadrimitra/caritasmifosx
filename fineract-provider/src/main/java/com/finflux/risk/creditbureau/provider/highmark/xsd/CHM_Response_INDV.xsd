<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
 <xs:element name="INDV-REPORT-FILE">
  <xs:complexType>
   <xs:sequence>
    <xs:element minOccurs="0" maxOccurs="1" ref="INQUIRY-STATUS"/>
    <xs:element minOccurs="0" maxOccurs="1" ref="INDV-REPORTS"/>
   </xs:sequence>
  </xs:complexType>
 </xs:element>
 
 <xs:element name="INQUIRY-STATUS">
  <xs:complexType>
  <xs:sequence>
   <xs:element ref="INQUIRY" minOccurs="1" maxOccurs="1"/>
</xs:sequence>	
  </xs:complexType>
 </xs:element>
  
<xs:element name="INQUIRY" >
	<xs:complexType>
	<xs:all>
		<xs:element name="INQUIRY-UNIQUE-REF-NO" type="xs:string" minOccurs="1" maxOccurs="1"/>
		<xs:element name="REQUEST-DT-TM" type="xs:string" minOccurs="1" maxOccurs="1"/>
		<xs:element name="RESPONSE-DT-TM" type="xs:string" minOccurs="1" maxOccurs="1"/>
		<xs:element name="RESPONSE-TYPE" type="xs:string" minOccurs="1" maxOccurs="1"/>
		<xs:element name="DESCRIPTION" type="xs:string" minOccurs="1" maxOccurs="1"/>
	</xs:all>	
	 </xs:complexType>
</xs:element>
		
 <xs:element name="INDV-REPORTS">
  <xs:complexType>
  <xs:sequence>
   <xs:element ref="INDV-REPORT" minOccurs="1" maxOccurs="1"/>
   </xs:sequence>
  </xs:complexType>
 </xs:element>
 
 <xs:element name="INDV-REPORT">
  <xs:complexType>
  <xs:all>
    <xs:element ref="HEADER" minOccurs="1" maxOccurs="1"/>
	<xs:element ref="STATUS-DETAILS" minOccurs="1" maxOccurs="1"/>
    <xs:element ref="REQUEST" minOccurs="1" maxOccurs="1"/>
    <xs:element ref="PERSONAL-INFO-VARIATION" minOccurs="0" maxOccurs="1"/>
    <xs:element ref="SECONDARY-MATCHES" minOccurs="0" maxOccurs="1"/>
    <xs:element ref="ACCOUNTS-SUMMARY" minOccurs="0" maxOccurs="1"/>
    <xs:element ref="RESPONSES" minOccurs="0" maxOccurs="1"/>
	<xs:element ref="INDV-RESPONSES" minOccurs="0" maxOccurs="1"/>
	<xs:element ref="GRP-RESPONSES" minOccurs="0" maxOccurs="1"/>
    <xs:element ref="INQUIRY-HISTORY" minOccurs="0" maxOccurs="1"/>
	<xs:element ref="COMMENTS" minOccurs="0" maxOccurs="1"/>
	<xs:element ref="ALERTS" minOccurs="0" maxOccurs="1"/>
	<xs:element ref="SCORES" minOccurs="0" maxOccurs="1"/>
	<xs:element ref="PRINTABLE-REPORT" minOccurs="0" maxOccurs="1"/>
	<xs:element ref="DERIVED-ATTRIBUTES" minOccurs="0" maxOccurs="1"/>
   </xs:all>
   </xs:complexType>
 </xs:element>
 
 
 <xs:element name="DERIVED-ATTRIBUTES" >
	   <xs:complexType>
		<xs:sequence>
	    <xs:element name="DERIVED-ATTRIBUTE" minOccurs="0" maxOccurs="unbounded" >
	   <xs:complexType>
		<xs:sequence>
	   <xs:element name="Name" type="xs:string" minOccurs="1" maxOccurs="1"/>
	   <xs:element name="Value" type="xs:string" minOccurs="1" maxOccurs="1"/>
	   <xs:element name="Remark" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
	   </xs:sequence>
    </xs:complexType>
	   </xs:element>
	   </xs:sequence>
    </xs:complexType>
	   </xs:element>
 
 
 <xs:element name="HEADER">
  <xs:complexType>
  <xs:all>
	<xs:element name="DATE-OF-REQUEST" type="xs:string" minOccurs="1" maxOccurs="1"/>
	<xs:element name="PREPARED-FOR" type="xs:string" minOccurs="1" maxOccurs="1"/>
	<xs:element name="PREPARED-FOR-ID" type="xs:string" minOccurs="0" maxOccurs="1"/>
	<xs:element name="DATE-OF-ISSUE" type="xs:string" minOccurs="1" maxOccurs="1"/>
	<xs:element name="REPORT-ID" type="xs:string" minOccurs="1" maxOccurs="1"/>
	<xs:element name="BATCH-ID" type="xs:string" minOccurs="1" maxOccurs="1"/>
	<!--
	<xs:element name="STATUS" type="xs:string" minOccurs="1" maxOccurs="1"/>
	<xs:element name="ERROR" type="xs:string" minOccurs="0" maxOccurs="1"/>
	-->
</xs:all>	
  </xs:complexType>
 </xs:element>
 <xs:element name="STATUS-DETAILS">
	<xs:complexType>
	<xs:sequence>
		<xs:element name="STATUS" minOccurs="1" maxOccurs="unbounded">
		  <xs:complexType>
		  <xs:all>
			<xs:element name="OPTION" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="OPTION-STATUS" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ERRORS" minOccurs="0" maxOccurs="1" >
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ERROR" minOccurs="1" maxOccurs="unbounded" >
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:string" name="ERROR-DESCRIPTION"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
		 </xs:all>		
		  </xs:complexType>
		</xs:element>
	</xs:sequence>
	</xs:complexType>
 </xs:element>

 <xs:element name="IDS">
	<xs:complexType>
	<xs:sequence>
		<xs:element name="ID" minOccurs="1" maxOccurs="unbounded">
		  <xs:complexType>
		  <xs:all>
			<xs:element name="TYPE" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="VALUE" type="xs:string" minOccurs="1" maxOccurs="1"/>
			
		  </xs:all>		
		  </xs:complexType>
		</xs:element>
	</xs:sequence>
	</xs:complexType>
 </xs:element>

 
 <xs:element name="ADDRESSES"  >
		<xs:complexType>
		<xs:sequence>
		<xs:element name="ADDRESS" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		</xs:complexType>
		</xs:element>
	<xs:element name="PHONES"  >
		<xs:complexType>
		<xs:sequence>
		<xs:element name="PHONE" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		</xs:complexType>
		</xs:element>	
		
		<xs:element name="EMAILS"  >
		<xs:complexType>
		<xs:sequence>
		<xs:element name="EMAIL" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		</xs:complexType>
		</xs:element>
		
		<xs:element name="RELATIONS" >
	         <xs:complexType>
	          <xs:sequence>
		       <xs:element name="RELATION" minOccurs="0" maxOccurs="unbounded">
		       <xs:complexType>
		        <xs:all>
			     <xs:element name="TYPE" type="xs:string" minOccurs="1" maxOccurs="1"/>
			     <xs:element name="NAME" type="xs:string" minOccurs="1" maxOccurs="1"/>
			      </xs:all>		
		    </xs:complexType>
		   </xs:element>
	       </xs:sequence>
	       </xs:complexType>
        </xs:element>
