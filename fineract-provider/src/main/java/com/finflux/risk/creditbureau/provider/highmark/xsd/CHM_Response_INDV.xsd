		<xs:element name="CREDIT-RPT-TRN-DT-TM" type="xs:string" minOccurs="0" maxOccurs="1"/>
		<xs:element name="AC-OPEN-DT" type="xs:string" minOccurs="0" maxOccurs="1"/>
		<xs:element name="LOAN-AMOUNT" type="xs:string" minOccurs="0" maxOccurs="1"/>
		<xs:element name="ENTITY-ID" type="xs:string" minOccurs="0" maxOccurs="1"/>
</xs:all>		
  
  </xs:complexType>
 </xs:element>
 
   <xs:complexType name="VARIATION">
  <xs:all>
   <xs:element name="VALUE" type="xs:string" minOccurs="1" maxOccurs="1"/>
