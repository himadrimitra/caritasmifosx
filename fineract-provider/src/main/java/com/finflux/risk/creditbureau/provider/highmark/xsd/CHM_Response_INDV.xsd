		<xs:element name="MBR-ID" type="xs:string" minOccurs="0" maxOccurs="1"/>
		<xs:element name="LOS-APP-ID" type="xs:string" minOccurs="0" maxOccurs="1"/>
		<xs:element name="CREDIT-INQ-PURPS-TYP" type="xs:string" minOccurs="0" maxOccurs="1"/>
